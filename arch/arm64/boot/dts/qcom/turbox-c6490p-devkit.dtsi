&i2c0 {
	status = "ok";
	es8316: es8316@11 {
		compatible = "everest,es8316";
		reg = <0x11>;
		clocks = <&lpass_aon LPASS_AON_CC_TX_MCLK_CLK>;
        clock-names = "mclk";
		#sound-dai-cells = <0>;
		power_en-gpio = <&tlmm 117 0>;
		irq-gpio = <&tlmm 63 0>;
		status = "ok";
    };
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&qup_i2c1_data_clk>;

	/delete-node/ typec-mux@42;

	typec-mux@42 {
		status = "disabled";
	};

	pm8008i@8 {
		status = "disabled";
	};

	pm8008i@9 {
		status = "disabled";
	};

	pm8008j@c {
		status = "disabled";
	};

	pm8008j@d {
		status = "disabled";
	};
};

